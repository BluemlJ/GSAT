package sequenceAnalyser;

/**
 * This class models a reference gene.
 *
 */
public class Gene extends Sequence {

    /**
     * Name of the known gene.
     */
    private String name;
    
    
    
    /**
     * Constructor setting attributes
     */
    public Gene(String sequence, String name) {
	super(sequence);
	this.name = name;
    }

}
